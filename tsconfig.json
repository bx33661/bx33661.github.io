{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "strictNullChecks": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  },
  "exclude": [
    "dist",
    "portfolio",
    "portfolio/**",
    "astro-theme-terminal",
    "astro-theme-terminal/**",
    "public/vendor/cyberchef",
    "public/vendor/cyberchef/**",
    ".tmp",
    ".tmp/**"
  ]
}
